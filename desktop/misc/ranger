set preview_images true
default_linemode devicons
set draw_borders true

map gd cd ~/downloads
map gs cd ~/other/screens
map go cd ~/other
map gC cd ~/.config
map gl cd ~/.local
map gp cd ~/build/
map gt cd ~/tmp/

map xx shell -fs termite
map xt shell -fs thunar
map xn shell -fs termite -e "zsh -c 'sleep 0.06 && nvim %s; zsh -i'"
map xd shell -fs dragon-drag-and-drop %p
